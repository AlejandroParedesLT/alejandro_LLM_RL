#include <iostream>
#include <vector>
#include <string>
#include <climits>

using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;

    while (t--) {
        int n;
        string s;
        cin >> n >> s;

        int min_result = INT_MAX;

        for (int i = 0; i < n - 1; ++i) {
            vector<int> nums;
            int j = 0;
            while (j < n) {
                if (j == i) {
                    int num = (s[j] - '0') * 10 + (s[j + 1] - '0');
                    nums.push_back(num);
                    j += 2;
                } else {
                    nums.push_back(s[j] - '0');
                    j += 1;
                }
            }

            bool has_zero = false;
            for (int num : nums) {
                if (num == 0) {
                    has_zero = true;
                    break;
                }
            }

            if (has_zero) {
                min_result = min(min_result, 0);
                continue;
            }

            long long sum = 0, product = 1;
            for (int num : nums) {
                sum += num;
                product *= num;
            }

            min_result = min(min_result, (int)min(sum, product));
        }

        cout << min_result << '\n';
    }

    return 0;
}
