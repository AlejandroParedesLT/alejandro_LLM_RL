typedef long long ll;

struct Edge {
    int to;
    ll weight;
};

pair<ll, int> bfs(int start, const vector<vector<Edge>>& adj, vector<int>& parent, vector<ll>& edge_weight, vector<ll>& dist) {
    int n = adj.size() - 1;
    parent.assign(n+1, -1);
    edge_weight.assign(n+1, 0);
    dist.assign(n+1, 0);
    queue<int> q;
    q.push(start);
    parent[start] = -1;
    dist[start] = 0;

    while (!q.empty()) {
        int u = q.front();
        q.pop();

        for (const Edge& e : adj[u]) {
            int v = e.to;
            ll w = e.weight;

            if (parent[u] == v) continue;

            if (parent[v] == -1) {
                parent[v] = u;
                edge_weight[v] = w;
                dist[v] = dist[u] + w;
                q.push(v);
            }
        }
    }

    ll max_dist = 0;
    int far_node = start;
    for (int i = 1; i <= n; ++i) {
        if (dist[i] > max_dist) {
            max_dist = dist[i];
            far_node = i;
        }
    }

    return {max_dist, far_node};
}

vector<tuple<int, int, ll>> get_edges_in_path(int start, int end, const vector<int>& parent, const vector<ll>& edge_weight) {
    vector<tuple<int, int, ll>> edges;
    int curr = end;

    while (curr != start) {
        int p = parent[curr];
        if (p == -1) break;
        edges.emplace_back(p, curr, edge_weight[curr]);
        curr = p;
    }

    reverse(edges.begin(), edges.end());
    return edges;
}

