#include <bits/stdc++.h>
using namespace std;

const int MAX_BITS = 30;

void solve() {
    int n;
    cin >> n;
    vector<int> x(n), y(n);
    for (int i = 0; i < n; ++i) {
        cin >> x[i] >> y[i];
    }
    
    vector<vector<int>> prefix(MAX_BITS, vector<int>(n+1, 0));
    for (int b = 0; b < MAX_BITS; ++b) {
        int val = 1 << b;
        for (int i = 0; i < n; ++i) {
            bool ok = false;
            if (x[i] > val) {
                if ((x[i] & val) && x[i] <= y[i]) {
                    ok = true;
                }
            } else {
                if (val <= y[i]) {
                    ok = true;
                }
            }
            prefix[b][i+1] = prefix[b][i] + ok;
        }
    }
    
    int q;
    cin >> q;
    while (q--) {
        int l, r;
        cin >> l >> r;
        int mask = 0;
        for (int b = MAX_BITS - 1; b >= 0; --b) {
            int cnt = prefix[b][r] - prefix[b][l-1];
            if (cnt > 0) {
                mask |= (1 << b);
            }
        }
        cout << mask << ' ';
    }
    cout << '\n';
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    
    int t;
    cin >> t;
    while (t--) {
        solve();
    }
    
    return 0;
}
