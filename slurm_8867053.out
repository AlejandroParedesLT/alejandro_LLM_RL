#include <bits/stdc++.h>
using namespace std;

struct SparseTableMax {
    vector<vector<int>> st;
    vector<int> log_table;

    SparseTableMax(const vector<int>& a) {
        int n = a.size();
        log_table.resize(n + 1);
        log_table[0] = log_table[1] = 0;
        for (int i = 2; i <= n; i++)
            log_table[i] = log_table[i / 2] + 1;
        int k = log_table[n] + 1;
        st.resize(k, vector<int>(n));
        for (int i = 0; i < n; i++)
            st[0][i] = i;
        for (int j = 1; j < k; j++)
            for (int i = 0; i + (1 << j) <= n; i++) {
                int m = i + (1 << (j - 1));
                int x = st[j-1][i];
                int y = st[j-1][m];
                st[j][i] = a[x] > a[y] ? x : y;
            }
    }

    int query(int l, int r, const vector<int>& a) {
        if (l > r) return -1;
        int len = r - l + 1;
        int k = log_table[len];
        int x = st[k][l];
        int y = st[k][r - (1 << k) + 1];
        return a[x] > a[y] ? x : y;
    }
};

struct SparseTableMin {
    vector<vector<int>> st;
    vector<int> log_table;

    SparseTableMin(const vector<int>& a) {
        int n = a.size();
        log_table.resize(n + 1);
        log_table[0] = log_table[1] = 0;
        for (int i = 2; i <= n; i++)
            log_table[i] = log_table[i / 2] + 1;
        int k = log_table[n] + 1;
        st.resize(k, vector<int>(n));
        for (int i = 0; i < n; i++)
            st[0][i] = i;
        for (int j = 1; j < k; j++)
            for (int i = 0; i + (1 << j) <= n; i++) {
                int m = i + (1 << (j - 1));
                int x = st[j-1][i];
                int y = st[j-1][m];
                st[j][i] = a[x] < a[y] ? x : y;
            }
    }

    int query(int l, int r, const vector<int>& a) {
        if (l > r) return -1;
        int len = r - l + 1;
        int k = log_table[len];
        int x = st[k][l];
        int y = st[k][r - (1 << k) + 1];
        return a[x] < a[y] ? x : y;
    }
};

void solve() {
    int n;
    cin >> n;
    vector<int> a(n);
    for (int& x : a) cin >> x;

    vector<int> next_smaller(n, n);
    stack<int> st;
    for (int i = n-1; i >= 0; i--) {
        while (!st.empty() && a[st.top()] >= a[i]) st.pop();
        if (!st.empty()) next_smaller[i] = st.top();
        st.push(i);
    }

    vector<int> next_greater(n, n);
    stack<int> gt;
    for (int i = n-1; i >= 0; i--) {
        while (!gt.empty() && a[gt.top()] <= a[i]) gt.pop();
        if (!gt.empty()) next_greater[i] = gt.top();
        gt.push(i);
    }

    SparseTableMax st_max(a);
    SparseTableMin st_min(a);

    vector<int> dist(n, -1);
    queue<int> q;
    dist[0] = 0;
    q.push(0);

    while (!q.empty()) {
        int u = q.front();
        q.pop();

        if (u == n-1) {
            cout << dist[u] << '\n';
            return;
        }

        // Case 1: a[u] is min, a[j] is max
        int s = next_smaller[u];
        int l = u + 1;
        int r = s - 1;
        if (l <= r) {
            int j_max = st_max.query(l, r, a);
            if (j_max != -1 && dist[j_max] == -1) {
                dist[j_max] = dist[u] + 1;
                q.push(j_max);
            }
        }

        // Case 2: a[u] is max, a[j] is min
        int t = next_greater[u];
        l = u + 1;
        r = t - 1;
        if (l <= r) {
            int j_min = st_min.query(l, r, a);
            if (j_min != -1 && dist[j_min] == -1) {
                dist[j_min] = dist[u] + 1;
                q.push(j_min);
            }
        }
    }
}

