const int INF = 1e9;

struct SegmentTree {
    vector<int> tree;
    int n;

    SegmentTree(const vector<int>& data) {
        n = 1;
        while (n < data.size()) n <<= 1;
        tree.resize(2 * n, INF);
        for (int i = 0; i < data.size(); ++i)
            tree[n + i] = data[i];
        for (int i = n - 1; i > 0; --i)
            tree[i] = min(tree[2 * i], tree[2 * i + 1]);
    }

    int query(int l, int r) { // [l, r]
        int res = INF;
        l += n;
        r += n;
        while (l <= r) {
            if (l % 2 == 1) res = min(res, tree[l++]);
            if (r % 2 == 0) res = min(res, tree[r--]);
            l >>= 1; r >>= 1;
        }
        return res;
    }
};

