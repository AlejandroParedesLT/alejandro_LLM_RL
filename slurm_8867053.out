#include <bits/stdc++.h>
using namespace std;

struct Gadget {
    int type, ci, index;
};

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n, m, k, s;
    cin >> n >> m >> k >> s;

    vector<int> a(n), b(n);
    for (int &x : a) cin >> x;
    for (int &x : b) cin >> x;

    vector<Gadget> gadgets(m);
    for (int i = 0; i < m; ++i) {
        cin >> gadgets[i].type >> gadgets[i].ci;
        gadgets[i].index = i + 1;
    }

    vector<int> min_a(n), day_a(n);
    min_a[0] = a[0];
    day_a[0] = 1;
    for (int i = 1; i < n; ++i) {
        if (a[i] < min_a[i-1]) {
            min_a[i] = a[i];
            day_a[i] = i + 1;
        } else {
            min_a[i] = min_a[i-1];
            day_a[i] = day_a[i-1];
        }
    }

    vector<int> min_b(n), day_b(n);
    min_b[0] = b[0];
    day_b[0] = 1;
    for (int i = 1; i < n; ++i) {
        if (b[i] < min_b[i-1]) {
            min_b[i] = b[i];
            day_b[i] = i + 1;
        } else {
            min_b[i] = min_b[i-1];
            day_b[i] = day_b[i-1];
        }
    }

    int left = 1, right = n, ans = -1;
    while (left <= right) {
        int mid = (left + right) / 2;
        int a_min = min_a[mid-1], b_min = min_b[mid-1];
        priority_queue<long long> pq;
        long long total = 0;

        for (const auto &g : gadgets) {
            long long cost = (g.type == 1) ? 1LL * g.ci * a_min : 1LL * g.ci * b_min;
            if (pq.size() < k) {
                pq.push(cost);
                total += cost;
            } else if (cost < pq.top()) {
                total -= pq.top() - cost;
                pq.pop();
                pq.push(cost);
            }
        }

        if (pq.size() == k && total <= s) {
            ans = mid;
            right = mid - 1;
        } else {
            left = mid + 1;
        }
    }

    if (ans == -1) {
        cout << -1 << endl;
        return 0;
    }

    int d = ans;
    int a_day = day_a[d-1], b_day = day_b[d-1];
    int a_min = min_a[d-1], b_min = min_b[d-1];
    vector<tuple<long long, int, int>> selected;

    for (const auto &g : gadgets) {
        long long cost = (g.type == 1) ? 1LL * g.ci * a_min : 1LL * g.ci * b_min;
        selected.emplace_back(cost, g.index, g.type);
    }

    sort(selected.begin(), selected.end());
    cout << d << '\n';
    for (int i = 0; i < k; ++i) {
        auto [cost, idx, type] = selected[i];
        cout << idx << ' ' << (type == 1 ? a_day : b_day) << '\n';
    }

    return 0;
}
