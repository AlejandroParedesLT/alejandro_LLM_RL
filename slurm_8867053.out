#include <bits/stdc++.h>
using namespace std;

int gcd(int a, int b) {
    while (b) {
        a %= b;
        swap(a, b);
    }
    return a;
}

vector<vector<int>> adj, adj_rev;
vector<bool> visited;
vector<int> order, component;

void dfs1(int u) {
    visited[u] = true;
    for (int v : adj[u]) {
        if (!visited[v]) {
            dfs1(v);
        }
    }
    order.push_back(u);
}

void dfs2(int u) {
    visited[u] = true;
    component.push_back(u);
    for (int v : adj_rev[u]) {
        if (!visited[v]) {
            dfs2(v);
        }
    }
}

vector<vector<int>> get_scc(int n) {
    visited.assign(n + 1, false);
    order.clear();
    for (int i = 1; i <= n; ++i) {
        if (!visited[i]) {
            dfs1(i);
        }
    }
    reverse(order.begin(), order.end());
    visited.assign(n + 1, false);
    vector<vector<int>> scc;
    for (int u : order) {
        if (!visited[u]) {
            component.clear();
            dfs2(u);
            scc.push_back(component);
            for (int v : component) {
                visited[v] = true;
            }
        }
    }
    return scc;
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);

    int n, m;
    cin >> n >> m;
    adj.resize(n + 1);
    adj_rev.resize(n + 1);
    for (int i = 0; i < m; ++i) {
        int a, b;
        cin >> a >> b;
        adj[a].push_back(b);
        adj_rev[b].push_back(a);
    }

    vector<vector<int>> scc = get_scc(n);
    vector<int> component_id(n + 1, -1);
    for (int i = 0; i < scc.size(); ++i) {
        for (int u : scc[i]) {
            component_id[u] = i;
        }
    }

    int global_gcd = 0;

    for (const auto &comp : scc) {
        if (comp.size() == 1) {
            int u = comp[0];
            bool has_self_loop = false;
            for (int v : adj[u]) {
                if (v == u) {
                    has_self_loop = true;
                    break;
                }
            }
            if (has_self_loop) {
                global_gcd = gcd(global_gcd, 1);
            }
        } else {
            int u = comp[0];
            vector<int> dist(n + 1, -1);
            queue<int> q;
            dist[u] = 0;
            q.push(u);
            while (!q.empty()) {
                int v = q.front();
                q.pop();
                for (int w : adj[v]) {
                    if (component_id[w] != component_id[v]) continue;
                    if (dist[w] == -1) {
                        dist[w] = dist[v] + 1;
                        q.push(w);
                    }
                }
            }
            int period_gcd = 0;
            for (int v : comp) {
                for (int w : adj[v]) {
                    if (component_id[w] != component_id[v]) continue;
                    if (dist[v] == -1 || dist[w] == -1) continue;
                    int current_diff = (dist[v] + 1) - dist[w];
                    period_gcd = gcd(period_gcd, abs(current_diff));
                }
            }
            if (period_gcd == 0) {
                for (int v : comp) {
                    for (int w : adj[v]) {
                        if (w == v) {
                            period_gcd = 1;
                            break;
                        }
                    }
                    if (period_gcd != 0) break;
                }
            }
            if (period_gcd != 0) {
                global_gcd = gcd(global_gcd, period_gcd);
            }
        }
    }

    if (global_gcd == 0) {
        cout << n << '\n';
    } else {
        cout << global_gcd << '\n';
    }

    return 0;
}
