#include <bits/stdc++.h>
using namespace std;

struct DSU {
    vector<int> parent, rank;
    int count;

    void reset(int n) {
        parent.resize(n+1);
        iota(parent.begin(), parent.end(), 0);
        rank.assign(n+1, 0);
        count = n;
    }

    int find(int x) {
        return parent[x] == x ? x : parent[x] = find(parent[x]);
    }

    void unite(int x, int y) {
        x = find(x);
        y = find(y);
        if (x == y) return;
        if (rank[x] < rank[y]) {
            parent[x] = y;
        } else {
            parent[y] = x;
            if (rank[x] == rank[y])
                rank[x]++;
        }
        count--;
    }
};

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n, m;
    cin >> n >> m;

