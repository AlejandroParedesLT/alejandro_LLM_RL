        if x == 3:\n            return \'—Ç—Ä–æ–µ –∫–æ–º–º–æ–≤ –∏ –æ–¥–∏–Ω —ç—Ç–∞–∂\'\n        if x == 4:\n            return \'—á–µ—Ç—ã—Ä–µ –∫–æ–º–º–æ–≤ –∏ –æ–¥–∏–Ω —ç—Ç–∞–∂\'\n        if x == 5:\n            return \'–ø—è—Ç—ã–µ –∫–æ–º–º–æ–≤ –∏ –æ–¥–∏–Ω —ç—Ç–∞–∂\'\n        if x == 6:\n            return \'—à–µ—Å—Ç–æ–µ –∫–æ–º–º–æ–≤ –∏—Ç—Ä–µ—Ç—ã —ç—Ç–∞–∂\'\n\n\n    # Enumerate all case combinations\n    def twoeps(n, m, k, a, e, f):\n        """Case for two apartments (OPS)"""\n        if n, m, k and a, e, f are all between 0 and 2:\n            comm1, floor1, comm2, menu1 = menu\n            comm2, floor2, comm1, menu1 = menu\n            comm1, floor1, comm2, menu1 = twoeps(n // 2, m, k, a, e,\n                k // n // 2) + menu\n            comm1, floor1, comm2, menu1 = twoeps(n // 2, m, k, a, e //\n                n // 2, e // m)\n            comm2, floor2, comm1, menu2 = twoeps(n, m // 2, k, lng(n // e //\n                2, m // f) * k // n // 2,_lng(m // f, e // n // 2), lng(n // f,\n                m // e))\n            comm1, floor1, comm2, menu1 = twoeps(n, m // 2, k, lng(n // e //\n                2, m // f) * k // n // 2,_lng(m // f, e // n // 2), lng(n // f,\n                m // e))\n            comm1, floor1, comm2, menu1 = twoeps(n, m, k, lng(n // e *\n                m // f, m // f) * k // n,_lng(m // f, e // n // 2), lng(n // f,\n                m // e))\n            comm1, floor1, comm2, menu1 = twoeps(n // 2, m, k,.lng(n == e *\n                n == m, m // f) * k // n,_lng(m == e, n // m % 0), lng(n == e,\n                m == e))\n            comm1, floor1, comm2, menu1 = twoeps(n, m, k, lng(n == e // 2, m ==\n                m // 2 * n % n == 0) * k // n,_lng(m == e, n // m % 0), lng(\n                n, m // 2 * n % n == 0))\n        else:\n            comm1, floor1, comm2, menu1 = solve(n, m, k, a, e, f)\n            comm1, floor1, comm2, menu1 = twoeps(n, m, k, n // 2, a, e)\n        return comm1, floor1, comm2, menu1\n\n\n    # Define menu