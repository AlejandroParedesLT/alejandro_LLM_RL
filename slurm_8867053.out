#include <bits/stdc++.h>
using namespace std;

int main() {
    int k, M;
    cin >> k >> M;

    vector<long long> fact(k + 1);
    vector<long long> pow_n(k + 1);
    fact[0] = 1;
    for (int i = 1; i <= k; ++i) {
        fact[i] = fact[i-1] * i % M;
    }

    for (int n = 1; n <= k; ++n) {
        if (n == 1) {
            cout << "0\n";
            continue;
        }
        // Compute (n-1)^n mod M
        long long a = n-1;
        long long res = 1;
        long long p = n;
        while (p) {
            if (p & 1) res = res * a % M;
            a = a * a % M;
            p >>= 1;
        }
        long long ans = fact[n] * res % M;
        cout << ans << '\n';
    }

    return 0;
}
```

