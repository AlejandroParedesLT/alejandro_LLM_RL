                    removed.push_back(idx + 1);
                }

                // Range after post
                for (int idx = post_end; idx < m; ++idx) {
                    removed.push_back(idx + 1);
                }

                // Add remaining flowers not in pre, mid, post
                cout << removed.size() << '\n';
                if (!removed.empty()) {
                    for (int x : removed) cout << x << ' ';
                    cout << '\n';
                }
