                    continue;
                }

                int pre_end = pre_count - 1;
                int mid_start = start;
                int mid_end = end;
                int post_start = end + 1;
                int post_end = post_start + post_count - 1;

                vector<int> removed;

                // Range between pre and mid
                for (int idx = pre_end + 1; idx < mid_start; ++idx) {
                    removed.push_back(idx + 1);
                }

