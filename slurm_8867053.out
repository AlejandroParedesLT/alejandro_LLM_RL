                return 0;
            }
            if (i != j && (d[i][j] <= 0)) {
                cout << "NO\n";
                return 0;
            }
        }
    }

    if (n == 1) {
        cout << "YES\n";
        return 0;
    }

    vector<int> parent(n, -1);
    parent[0] = 0;

    for (int i = 1; i < n; ++i) {
        int best_j = -1;
        int max_d0j = -1;
        for (int j = 0; j < n; ++j) {
            if (j == i) continue;
            if (d[0][j] + d[j][i] == d[0][i] && d[j][i] > 0) {
                if (d[0][j] > max_d0j) {
